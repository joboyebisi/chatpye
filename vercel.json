{
  "version": 2,
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "chatpye.com"
        }
      ],
      "destination": "https://app.chatpye.com",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "chatpyeyoutube.com"
        }
      ],
      "destination": "https://app.chatpye.com",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "chatpye.com"
        }
      ],
      "destination": "https://app.chatpye.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "chatpyeyoutube.com"
        }
      ],
      "destination": "https://app.chatpye.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 