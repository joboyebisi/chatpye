{
  "version": 2,
  "redirects": [
    {
      "source": "https://chatpye.com",
      "destination": "https://app.chatpye.com",
      "permanent": true
    },
    {
      "source": "https://chatpyeyoutube.com",
      "destination": "https://app.chatpye.com",
      "permanent": true
    },
    {
      "source": "http://chatpye.com",
      "destination": "https://app.chatpye.com",
      "permanent": true
    },
    {
      "source": "http://app.chatpye.com",
      "destination": "https://app.chatpye.com",
      "permanent": true
    },
    {
      "source": "http://chatpyeyoutube.com",
      "destination": "https://app.chatpye.com",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 